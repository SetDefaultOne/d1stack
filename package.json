{
    "name": "d1stack",
    "version": "0.1.0",
    "engineStrict": true,
    "engines": {
        "node": "22.12.0",
        "npm": "10.9.0"
    },
    "packageManager": "npm@10.9.0",
    "workspaces": [
        "apps/*",
        "libs/*",
        "packages/*",
        "configs/*"
    ],
    "devDependencies": {
        "@d1stack/prettier-config": "*"
    },
    "scripts": {
        "format": "prettier . --write --cache && turbo format",
        "lint": "prettier . --check --cache && turbo lint",
        "build": "turbo build",
        "dev": "turbo dev"
    }
}
