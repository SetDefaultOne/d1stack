{
    "name": "@d1stack/source",
    "version": "0.0.1",
    "devDependencies": {
        "@commitlint/cli": "19.6.0",
        "@commitlint/config-conventional": "19.6.0",
        "@commitlint/types": "19.5.0",
        "husky": "9.1.7",
        "prettier": "3.4.1",
        "syncpack": "13.0.0"
    },
    "scripts": {
        "format": "syncpack format && prettier --write --cache --no-error-on-unmatched-pattern --ignore-unknown .",
        "lint": "syncpack lint && prettier --check --cache --no-error-on-unmatched-pattern --ignore-unknown .",
        "prepare": "husky"
    }
}
